--bullets

function shoot()
 local new_bullets={
 x=p.x*8,
 y=p.y*8,
 speed=4
  }
 add(bullets, new_bullets)
 sfx(0)
 end
 
 function update_bullets()
  for b in all(bullets) do
   b.y-=b.speed
   if (b.y<-8) del(bullets, b)
  end
 end
