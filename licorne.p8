version 29
__lua__
-- jeu licorne
-- par aline, amanda, hortense

function _init()
 create_player()
 bullets={}
 enemies={}
 explosions={}
-- spawn_enemies()
end

function _update()
 player_movement()
 update_bullets()
 update_enemies()
 update_explosions()
-- update_camera()
 if #enemies==0 then
  spawn_enemies()
 end
end

function _draw()
cls()


--map
draw_map()

--licorne
draw_player()


--etoiles
for b in all(bullets) do
 spr(7,b.x,b.y)
 end

--enemies
for e in all(enemies) do
 spr(8,e.x,e.y)
end

--explosions
draw_explosions()

end


-->8
--bullets

function shoot()
 local new_bullets={
 x=p.x*8,
 y=p.y*8,
 speed=4
 }
 add(bullets, new_bullets)
 sfx(0)
 end
 
 function update_bullets()
  for b in all(bullets) do
   b.y-=b.speed
   if (b.y<-8) del(bullets, b)
  end
 end
-->8
--enemies

function spawn_enemies()
 add(enemies,{
 x=0,
 y=20,
 life=1
 }) 
 end

function update_enemies()
 for enemy in all(enemies) do
 enemy.x +=0.3
  if enemy.x > 130 then
  del(enemies,enemy)
  end

-- collision
  for b in all(bullets) do
   if collision(enemy,b) then
    create_explosion(b.x,b.y)
    sfx(1)
    del(bullets,b)
    enemy.life-=1
    if enemy.life==0 then
     del(enemies,enemy)
    end
   end
  end  
 end
end



-->8
--map

function draw_map()
 map(0,0,0,0,128,64)
end

function check_flag(flag,x,y)
 local sprite=mget(x,y)
 return fget(sprite,flag)
end

function update_camera()
 local camx=flr(p.x/16)*16
 local camy=flr(p.x/16)*16
 camera(p.x*8,p.y*8)
end
-->8
function create_player()
 p={
  x=1,
  y=2,
  sprite=50,
  life=1
 }
end

function player_movement()
 newx=p.x
 newy=p.y

 if (btnp(➡️)) newx+=1
 if (btnp(⬅️)) newx-=1
 if (btnp(⬆️)) newy-=1
 if (btnp(⬇️)) newy+=1
 if (btnp(❎)) shoot()
 
 if not check_flag(0,newx,newy) then
  p.x=mid(0,newx,127)
  p.y=mid(0,newy,63)
  end
 end

function draw_player()
 spr(p.sprite,p.x*8,p.y*8)
end

function update_player()
-- collision enemy
  for e in all(enemies) do
   if collision(p,b) then
--    sfx(1)
--    del(bullets,b)
    p.life-=1
    if p.life==0 then
     del(p)
     end
    end
   end
  end
 
 
-->8
-- tools

function collision(a,b)
 return not (a.x > b.x+8
 											 or a.y > b.y+8
 											 or a.x+8 < b.x
 											 or a.y+8 < b.y)
 end
-->8
-- explosions

function create_explosion(x,y)
 add(explosions,{
  x = x,
  y = y, 
  timer = 0
  })
 sfx(1)
end

function update_explosions()
 for boom in all(explosions) do
 boom.timer += 1
 if boom.timer == 13 then
  del(explosions, boom)
  end
 end
end

function draw_explosions()
 for boom in all(explosions) do
 circ(boom.x, boom.y,
  boom.timer/3, 8+boom.timer%3)
  end
 end
 

