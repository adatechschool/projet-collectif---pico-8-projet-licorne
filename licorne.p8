--enemies2

function spawn_enemies2(amount)
 gap=(128-8*amount)/(amount+1)
 for i=1,amount do
 add(enemies2,{
 x=gap*i + 8*(i+15.5),
 y=0,
 anim="walk",
 walk={f=38,st=38,sz=2,spd=1/5},
 life=2
   }) 
  end
 end

function update_enemies2()
 for enemy2 in all(enemies2) do
 enemy2.y +=0.6
   if enemy2.y >104 then
  del(enemies2,enemy2)
  end
 
-- collision
  for b in all(bullets) do
   if collision(enemy2,b) then
    create_explosion(b.x,b.y)
    del(bullets,b)
    enemy2.life-=1
    if enemy2.life==0 then
     del(enemies2,enemy2)
     sfx(5)
    end 		  
   end
  end  
 end
end

function colision_e2()
for e in all(enemies2) do
  if flr(e.y/8) == p.y then
 	if flr(e.x/8) == p.x then
 	
 	p.life -= 1
 	e.y -= 8
 	p.y += 2

-- 	 if p.life == 0 then
--     _init()
--   end
-- 	 end

 	sfx(4)
 	 
    end
  end
 end
end
