--enemies

function spawn_enemies(amount)
 gap=(128-8*amount)/(amount+1)
 for i=1,amount do
 add(enemies,{
 x=0,
 y=gap*i + 8*(i-2),
 anim="walk",
 walk={f=22,st=22,sz=2,spd=1/5},
  life=1
   }) 
  end
 end 

function update_enemies()
 for enemy in all(enemies) do
 enemy.x +=0.5
   if enemy.x > 120 then
  del(enemies,enemy)
  end
 
-- collision
  for b in all(bullets) do
   if collision(enemy,b) then
    create_explosion(b.x,b.y)
    del(bullets,b)
    enemy.life-=1
    if enemy.life==0 then
     del(enemies,enemy)
     sfx(5)
     end 
   end
  end  
 end
end




-->8
--map

function draw_map()
 map(0,0,0,0,128,64)
end

function check_flag(flag,x,y)
 local sprite=mget(x,y)
 return fget(sprite,flag)
end

function update_camera()
 local camx=flr(p.x/16)*16
 local camy=flr(p.y/16)*16
 camera(camx*8,camy*8)
end

--animation du feu
function swap_tile(x,y)
 tile=mget(x,y)
 mset(x,y,tile+1)
end

function unswap_tile(x,y)
 tile=mget(x,y)
 mset(x,y,tile-1)
end


-- arc en ciel

function next_tile(x,y)
 local sprite=mget(x,y)
 mset(x,y,sprite-1)
end

function arc_en_ciel(x,y)
 next_tile(x,y)
 p.keys+=1
end

--open door

function open_door(x,y)
 next_tile(x,y)
 p.keys-=3
end
