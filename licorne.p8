--player
function create_player()
 p = {
  x=1,
  y=13,
  sprite=50,
  life=3,
  keys=0,
  touch= "",
-- parametres animation de la licorne
-- f/frame st/start sz/size spd/speed
  anim="walk",
  wait={f=52,st=52,sz=2,spd=1/15},
  walk={f=49,st=49,sz=3,spd=1/5}
  }
end

function player_movement()

 p.anim = "walk" 
 
 newx=p.x
 newy=p.y

 if (btnp(➡️)) newx+=1 p.flip=true sfx()
 if (btnp(⬅️)) newx-=1 p.flip=false sfx()
 if (btnp(⬆️)) newy-=1 sfx()
 if (btnp(⬇️)) newy+=1 sfx()
 if (btnp(❎)) shoot()
 
 interact(newx,newy)
 
 if not check_flag(0,newx,newy) then
  p.x=mid(0,newx,127)
  p.y=mid(0,newy,63)
 
 --si aucun bouton appuie anim wait, sinon walk 
 if ((btn(➡️) or btn(⬅️) 
 or btn(⬆️) or btn(⬇️)) == false) then
 p.anim = "wait"

 
 for e in all(enemies) do
 	if flr(e.x/8) == p.x then
 	if flr(e.y/8) == p.y then
 		-- gestion de collision
 		p.life -= 1
 	 e.x -= 2
 	 p.x += 2
 	 sfx(4)
 	   end
 	  end
 	 end 
 	end
 end
end
 
function draw_player()
 spr(anim_player(),p.x*8,p.y*8,1,1,p.flip)
end

function interact(x,y)
 if check_flag(1,x,y) then 
  arc_en_ciel(x,y)
  sfx(2)
  elseif check_flag(2,x,y)
  and p.keys>2 then
 open_door(x,y)
 sfx(1)
 end
end

 
 
