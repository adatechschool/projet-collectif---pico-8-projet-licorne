-- tools

function collision(a,b)
 return not (a.x > b.x+8
 											 or a.y > b.y+8
 											 or a.x+8 < b.x
											 or a.y+8 < b.y)
 end

--animation

function anim(a)
 a.f += a.spd
 if (a.f >= a.st + a.sz) then
  a.f = a.st
 end
 return flr(a.f)
end

function anim_player()
 if (p.anim == "wait") then
  return anim(p.wait)
 else
  return anim(p.walk)
 end
end

-- user interface

function draw_ui()
 camera()
 palt(0,false)
 palt(12,true)
 spr(104,2,2)
 palt()
 print(" "..p.keys,10,2,7)
end

-- affichage des vies

function draw_life()
 camera()
 spr(8,120,0)
 print(p.life,115,0)
end

-- explosions

function create_explosion(x,y)
 add(explosions,{
  x = x,
  y = y, 
  timer = 0
  })
 sfx()
end

function update_explosions()
 for boom in all(explosions) do
 boom.timer += 1
 if boom.timer == 13 then
  del(explosions, boom)
  end
 end
end

function draw_explosions()
 for boom in all(explosions) do
 circ(boom.x, boom.y,
  boom.timer/3, 8+boom.timer%3)
  end
 end
 
